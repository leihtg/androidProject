<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:paddingLeft="20dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:background="@color/white"
    android:gravity="center_vertical">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_money_log_listitem_tradeMoney"
            android:text="河南省 洛阳市"
            style="@style/tv_list_bigContent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            />

        <TextView
            android:id="@+id/tv_money_log_listitem_time"
            style="@style/tv_list_time"
            android:text="11111111111111" />

    </LinearLayout>

    <TextView
        android:id="@+id/tv_money_log_listitem_availableMoney"
        style="@style/tv_list_content"
        android:text="登录成功" />

    <TextView
        android:id="@+id/tv_money_log_listitem_moneyTo"
        style="@style/tv_list_content"
        android:text="10.12.10.00"

        />


</LinearLayout>